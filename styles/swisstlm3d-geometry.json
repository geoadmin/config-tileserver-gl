{
	"version": 8,
	"name": "Test SwissTLM3d Geometry",
	"metadata": {
		"mapbox:autocomposite": false,
		"mapbox:type": "template",
		"maputnik:renderer": "mbgljs",
		"openmaptiles:version": "3.x"
	},
	"sources": {
		"swisstlm3d-geometry": {
			"type": "vector",
			"url": "mbtiles://swisstlm3d-geometry.mbtiles"
		}
	},
	"layers": [
	        {
                        "id": "Background",
                        "type": "background",
                        "paint": {
                                "background-color": "rgba(222, 222, 222, 1)"
                        }
                },
 	        {
			"id": "Gewaesser",
			"type": "fill",
			"source": "swisstlm3d-geometry",
			"source-layer": "TLM_BODENBEDECKUNG",
			"filter": [
				"any",
                                 [
					"==",
					"objektart",
					"10"
				],
	                        [
					"==",
					"objektart",
					"5"
				]
			],
	                "paint": {
                                "fill-color": "rgba(0, 122, 209, 1)"
			}
		},
	        {
			"id": "Wald",
			"type": "fill",
			"source": "swisstlm3d-geometry",
			"source-layer": "TLM_BODENBEDECKUNG",
			"filter": [
				"all", [
					"==",
					"objektart",
					"12"
				]
			],
                        "paint": {
                                "fill-color": "rgba(148, 224, 148, 1)"
                        }
		},
                {
			"id": "Gebaeude",
			"type": "fill",
			"source": "swisstlm3d-geometry",
			"source-layer": "TLM_GEBAEUDE_FOOTPRINT",
			"filter": [
				"all"
			],
			"paint": {
				"fill-color": "rgba(50, 50, 50, 1)"
			}
		}
	
	],
    "sprite": "swisstopo",
    "glyphs": "{fontstack}/{range}.pbf",
	"id": "swisstlm3d-geometrie",
	"owner": "swisstopo"
}
