{
  "version": 8,
  "name": "Psychedelic",
  "metadata": {
    "mapbox:autocomposite": false,
    "mapbox:type": "template",
    "maputnik:renderer": "mbgljs",
    "openmaptiles:version": "3.x"
  },
  "sources": {
    "swissbasemapzoom-mbtiles-id": {
      "type": "vector",
      "url": "https://tileserver.dev.bgdi.ch/data/swissbasemapzoom-mbtiles.json"
    },
    "relief": {
      "type": "raster",
      "url": "https://tileserver.dev.bgdi.ch/tilejson/relief.json"
    }
  },
  "sprite": "https://tileserver.dev.bgdi.ch/styles/psychedelic/sprite",
  "glyphs": "https://tileserver.dev.bgdi.ch/fonts/{fontstack}/{range}.pbf",
  "layers": [
    {
      "id": "background",
      "type": "background",
      "layout": {
        "visibility": "visible"
      },
      "paint": {
        "background-color": "rgba(80, 254, 48, 1)"
      }
    },
    {
      "id": "hillshade",
      "type": "raster",
      "source": "relief",
      "paint": {
        "raster-opacity": 0.2
      }
    },
    {
      "id": "lakes",
      "type": "fill",
      "source": "swissbasemapzoom-mbtiles-id",
      "source-layer": "swissbasemapzoom-layer",
      "filter": [
        "all",
        [
          "==",
          "layerid",
          "GEW_STEHENDGW_PLY"
        ]
      ],
      "layout": {
        "visibility": "visible"
      },
      "paint": {
        "fill-color": "rgba(234, 18, 18, 1)"
      }
    },
    {
      "id": "boundaries",
      "type": "line",
      "source": "swissbasemapzoom-mbtiles-id",
      "source-layer": "swissbasemapzoom-layer",
      "filter": [
        "all",
        [
          "==",
          "layerid",
          "ADM_CH-FL_PLY"
        ]
      ],
      "layout": {
        "visibility": "visible"
      },
      "paint": {
        "line-color": "rgba(18, 33, 232, 1)"
      }
    },
    {
      "id": "names",
      "type": "symbol",
      "source": "swissbasemapzoom-mbtiles-id",
      "source-layer": "swissbasemapzoom-layer",
      "minzoom": 0,
      "maxzoom": 24,
      "filter": [
        "all",
        [
          "==",
          "layerid",
          "SDL_ORT100T_LBP"
        ]
      ],
      "layout": {
        "text-field": "{name}",
        "text-font": [
          "Arial",
          "Arial Unicode MS Regular"
        ],
        "text-size": 16,
        "visibility": "visible",
        "text-letter-spacing": 2,
        "text-pitch-alignment": "auto",
        "text-optional": false,
        "text-transform": "uppercase",
        "text-allow-overlap": true,
        "text-ignore-placement": true,
        "text-justify": "center"
      },
      "paint": {
        "text-color": "rgba(255, 44, 236, 1)",
        "text-opacity": 1,
        "text-halo-color": "rgba(255, 170, 54, 1)",
        "text-halo-width": 50
      }
    }
  ],
  "id": "i10cuwh51"
}